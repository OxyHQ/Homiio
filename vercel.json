{
  "name": "homiio-monorepo",
  "buildCommand": "npm run build:shared-types && npm run build:frontend",
  "outputDirectory": "packages/frontend/dist",
  "installCommand": "npm install",
  "framework": null,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}